{
  "name": "my-backend",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/my-backend/src",
  "projectType": "library",
  "tags": [],
  "targets": {
    "amplify-init": {
      "command": "node amplify-init.js",
      "options": {
        "cwd": "libs/my-backend"
      }
    },
    "amplify-configure": {
      "command": "node amplify-configure.js",
      "options": {
        "cwd": "libs/my-backend"
      }
    },
    "amplify-pull": {
      "command": "node amplify-pull.js",
      "options": {
        "cwd": "libs/my-backend"
      }
    },
    "amplify-push": {
      "command": "node amplify-push.js",
      "options": {
        "cwd": "libs/my-backend"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"]
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "libs/my-backend/jest.config.ts"
      }
    }
  }
}
